{
  "name": "social-wall",
  "version": "0.0.0",
  "main": "Server/app.js",
  "private": true,
  "scripts": {
    "// -- Server tasks": "echo \"tasks for the backend environment\"",
    "dev": "nodemon --inspect Server/app.js --exec babel-node --presets babel-preset-env",
    "start": "node dist/index.js",
    "// -- Client tasks": "echo \"tasks for the frontend environment\"",
    "client:watch:all": "concurrently --names \"JS,SCSS\" -c \"blue,red,green\" \"yarn run client:watch:js\" \"yarn run client:watch:scss\"",
    "client:watch:js": "yarn run client:compile:js -- --watch",
    "client:watch:scss": "nodemon -e scss -w src/scss/ -x \"yarn run client:compile:scss\"",
    "client:build:all": "concurrently --names \"JS,SCSS\" -c \"blue,red,green\" \"yarn run client:compile:js\" \"yarn run client:compile:scss\"",
    "client:compile:js": "webpack --config webpack.config.js --progress",
    "client:compile:scss": "node sass.config.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "~1.2.9",
    "@fortawesome/free-solid-svg-icons": "~5.6.0",
    "@fortawesome/vue-fontawesome": "~0.1.3",
    "axios": "~0.18.0",
    "babel-core": "6.26.*",
    "babel-loader": "7.1.*",
    "babel-polyfill": "6.26.*",
    "body-parser": "~1.18.3",
    "concurrently": "~4.1.0",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "cssnano": "3.10.*",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-vue": "^4.7.1",
    "express": "~4.16.0",
    "http": "^0.0.0",
    "http-errors": "~1.6.2",
    "inline-style-prefixer": "^5.0.3",
    "jade": "~1.11.0",
    "mongoose": "~5.3.16",
    "mongoose-paginate": "git+http://github.com/zerok78/mongoose-paginate.git#version-5.0.4",
    "morgan": "~1.9.0",
    "node-sass": "~4.10.0",
    "node-sass-tilde-importer": "~1.0.2",
    "postcss": "6.0.*",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "sugarss": "^2.0.0",
    "uglifyjs-webpack-plugin": "1.1.*",
    "vue": "~2.5.18",
    "vue-good-table": "~2.16.0",
    "vue-loader": "^15.4.2",
    "vue-notification": "~1.3.14",
    "vue-router": "~3.0.2",
    "vue-template-compiler": "^2.5.17",
    "vuex": "~3.0.1",
    "webpack": "3.11.*"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.18.7",
    "webpack-dev-server": "~3.1.10"
  }
}
